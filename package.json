{
  "name": "restriction",
  "version": "1.0.0-release",
  "description": "The module of generated by vessel.",
  "keywords": [
    "restriction",
    "vessel",
    "debounce",
    "throttle",
    "front-end",
    "back-end"
  ],
  "homepage": "https://github.com/mornya/restriction",
  "license": "MIT",
  "author": "mornya <mornya78@gmail.com>",
  "private": false,
  "repository": "https://github.com/mornya/restriction",
  "bugs": "https://github.com/mornya/restriction/issues",
  "main": "./dist/index.js",
  "types": "./dist",
  "scripts": {
    "setup": "husky install && lintest install || exit 0",
    "prepublishOnly": "vessel prepublish",
    "login": "vessel login",
    "clean": "vessel clean",
    "build": "vessel build",
    "watch": "vessel build --watch",
    "format": "prettier --write --ignore-path .prettierignore ./src ./types",
    "check": "vessel check",
    "lint": "lintest lint",
    "lint:fix": "lintest lint --fix",
    "test": "lintest test",
    "test:watch": "lintest test --watch",
    "test:coverage": "lintest test --coverage",
    "sonar": "sonar-scanner"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "dist",
    "!dist/test",
    "!dist/**/@setup.d.ts",
    "!dist/**/@setup.js",
    "!dist/**/*.test.d.ts",
    "!dist/**/*.test.js",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "dependencies": {},
  "devDependencies": {
    "@lintest/core": "^1.13.8",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/webpack-env": "^1.18.1",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run check",
      "npm run lint"
    ]
  },
  "vessel": {
    "minVersion": "5.2.0"
  }
}
